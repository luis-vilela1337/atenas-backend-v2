    "@types/hash-it": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/multer": "^1.4.11",
    "@types/node": "^16.0.0",
    "@types/passport-http": "^0.3.9",
    "@types/pdfmake": "^0.2.8",
    "@types/pg": "^8.15.1",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^8.3.4",
