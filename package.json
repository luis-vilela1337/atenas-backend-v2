{
  "name": "back-core",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "pre-commit": "lint-staged",
    "build": "nest build",
    "start": "nest start",
    "start:dev": "rimraf dist && nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "test:webhook": "node webhook-test.js",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "test": "jest --config jest-unit-config.js",
    "test:watch": "jest --config jest-unit-config.js --watch",
    "test:coverage": "jest --config jest-unit-config.js --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand --config jest-unit-config.js",
    "test:integration": "jest --config jest-integration-config.js",
    "test:e2e": "jest --config test/jest-e2e.json",
    "test:all": "npm run test && npm run test:integration && npm run test:e2e",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli",
    "migration:run": "pnpm run typeorm migration:run -d ./src/infra/data/sql/database.config.ts",
    "migration:create": "pnpm run typeorm migration:create ./src/infra/data/migrations/migration",
    "migration:generate": "pnpm run typeorm migration:generate  -d ./src/infra/data/sql/database.config.ts ./src/infra/data/migrations/generated",
    "migration:down": "pnpm run typeorm migration:revert -d ./src/infra/data/sql/database.config.ts",
    "migration:show": "pnpm run typeorm migration:show -d ./src/infra/data/sql/database.config.ts"
  },
  "dependencies": {
    "@google-cloud/storage": "^7.13.0",
